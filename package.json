{
  "name": "admin-janda",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/start.js",
  "build": {
    "appId": "com.electron.electron-with-create-react-app",
    "win": {
      "iconUrl": "https://cdn2.iconfinder.com/data/icons/designer-skills/128/react-256.png"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "dependencies": {
    "@janda-com/front": "^2.0.27",
    "@types/classnames": "^2.2.10",
    "apollo": "^2.25.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-state": "^0.4.2",
    "apollo-upload-client": "^12.1.0",
    "apollo-utilities": "^1.3.3",
    "classnames": "^2.2.6",
    "electron": "^8.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "graphql-tag": "^2.10.3",
    "moment": "^2.24.0",
    "react": "^16.13.0",
    "react-apollo": "^3.1.3",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-toastify": "^5.5.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "yarn react-start",
    "origin-start": "react-scripts start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "react-app-rewired start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "install-app-deps",
    "precodegen:ticket": "apollo schema:download --endpoint=https://api.space.stayjanda.cloud/graphql",
    "codegen:ticket": "apollo codegen:generate src/types/apiTicket.d.ts --queries=src/apollo/queriesTicket.ts --addTypename --localSchemaFile=schema.json --target=typescript --outputFlat",
    "precodegen:b": "apollo schema:download --endpoint=https://temp-booking.stayjanda.cloud/graphql",
    "codegen:b": "apollo codegen:generate src/types/api.ts --queries=src/apollo/*.qr.ts --addTypename --localSchemaFile=schema.json --target=typescript --outputFlat"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "**/react": "16.13.0",
    "**/react-dom": "16.13.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/apollo-upload-client": "^8.1.3",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "customize-cra": "^0.9.1",
    "electron-builder": "^22.5.1",
    "node-sass": "^4.13.1",
    "react-app-rewired": "^2.1.5"
  }
}
